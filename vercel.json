{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
       
        {
            "src": "/(.*)",
            "dest": "/"
        },
        {
            "src": "/submit",
            "dest": "./index.js"
        }
    ]
}